<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
    <file original="file.ext" source-language="en" datatype="plaintext">
        <body>
            <trans-unit id="welcome.text">
                <source>Welcome %name%, in the Learning Center of the School %schoolname%.</source>
                <target>Wilkommen %name%, in der Lernumgebung der Schule %schoolname%.</target>
            </trans-unit>
            <trans-unit id="home">
                <source>Home</source>
                <target>Startseite</target>
            </trans-unit>
            <trans-unit id="catalog">
                <source>Catalog</source>
                <target>Lernkatalog</target>
            </trans-unit>
            <trans-unit id="files">
                <source>Filemanager</source>
                <target>Speicher</target>
            </trans-unit>
            <trans-unit id="projects">
                <source>Projects</source>
                <target>Projekte</target>
            </trans-unit>
            <trans-unit id="lists">
                <source>Lists</source>
                <target>Listen</target>
            </trans-unit>
            <trans-unit id="lc.invalidLogin">
                <source>Login failed (note that usernames and passwords are case-sensitive)!</source>
                <target>Anmeldung fehlgeschlagen (Benutzernamen und Passwörter unterscheiden zwischen Groß- und Kleinschreibung)!</target>
            </trans-unit>
            <trans-unit id="lc.accountLocked">
                <source>This account has been locked! You can log in again in %d minutes.</source>
                <target>Das Konto wurde gesperrt! Sie können sich in %d Minuten erneut anmelden.</target>
            </trans-unit>
            <trans-unit id="project.need.confirm">
                <source>project.need.confirm</source>
                <target>Das Projekt %name% muss noch vom Gruppenleiter bestätigt werden um die vollen Funktionen freizuschalten.</target>
            </trans-unit>
            <trans-unit id="project.confirmed">
                <source>project.confirmed</source>
                <target>Das Projekt %name% wurde vom Gruppenleiter bestätigt und besitzt nun die vollen Funktionen.</target>
            </trans-unit>
            <trans-unit id="project.confirm">
                <source>project.confirm</source>
                <target>Bestätigen</target>
            </trans-unit>
            <trans-unit id="project.confirm.confirm">
                <source>project.confirm.confirm</source>
                <target>Wollen Sie wirklich das Projekt %name% bestätigen?</target>
            </trans-unit>
            <trans-unit id="project.decline">
                <source>project.decline</source>
                <target>Ablehnen</target>
            </trans-unit>
            <trans-unit id="project.decline.confirm">
                <source>project.decline.confirm</source>
                <target>Wollen Sie wirklich das Projekt %name% ablehnen.</target>
            </trans-unit>
            <trans-unit id="project.create.member.message">
                <source>project.create.member.message</source>
                <target>Das Projekt wurde erstellt.</target>
            </trans-unit>
            <trans-unit id="project.create.member.message.need.confirm">
                <source>project.create.member.message.need.confirm</source>
                <target>Das Projekt wurde erfolgreich erstellt, jedoch um alle Funktionen frei zu schalten muss das Projekt noch vom Gruppenleiter bestätigt werden.</target>
            </trans-unit>
        </body>
    </file>

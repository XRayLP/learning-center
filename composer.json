{
	"name" : "xraylp/learning-center-bundle",
	"description" : "Learning Platform",
	"type" : "contao-bundle",
	"license" : "GPL-3.0-only",
	"minimum-stability" : "dev",
	"require" : {
		"php" : ">=7.0",
		"contao/core-bundle" : "^4.4",
		"knplabs/knp-menu-bundle": "^2.0",
		"lasserafn/php-initial-avatar-generator": "^2.3",
		"doctrine/orm": "^2.6"
	},
	"autoload" : {
		"psr-4" : {
			"XRayLP\\LearningCenterBundle\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"XRayLP\\LearningCenterBundle\\Test\\" : "tests"
		}
	},
	"extra" : {
		"contao-manager-plugin": "XRayLP\\LearningCenterBundle\\ContaoManagerPlugin"
	},
	"require-dev": {
		"symfony/phpunit-bridge": "^4.1@dev"
	}
}
